            </ui:VisualElement>
            <ui:ListView name="RegionList" horizontal-scrolling="true" style="width: auto; color: rgb(255, 255, 255); height: 650px; font-size: 16px; -unity-text-align: middle-left; white-space: nowrap; margin-top: 10px;" />
        </ui:VisualElement>
    </ui:VisualElement>
</ui:UXML>
